{
  "name": "react-2048",
  "version": "0.0.0",
  "description": "A clone of 2048 game with React",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config scripts/webpack.prod.js",
    "start": "webpack-dev-server --config scripts/webpack.dev.js",
    "start:host": "webpack-dev-server --config scripts/webpack.dev.js --host 0.0.0.0",
    "lint": "eslint . --ext .js,.ts,.tsx --max-warnings 0",
    "format": "prettier --write .",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "keywords": [
    "React",
    "2048",
    "JavaScript"
  ],
  "author": "Kai Wang",
  "license": "ISC",
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.1",
    "styled-normalize": "^8.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.14.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.0.0",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.5",
    "prettier": "^2.2.1",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2",
    "webpack": "^4.41.3",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
